/* This example demonstrates how the FaCT compiler transforms a conditional
 * branch on a secret value. The resulting executable contains no branches on
 * the secret cond, thereby closing a potential timing side-channel. */
export secret uint32 select (
    secret bool cond, 
    public uint32 x, 
    public uint32 y) {
  secret mut uint32 out = x;
  if(cond) {
      out = y;
  }
  return out;
}

